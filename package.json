{
  "name": "luhi-website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "animate.css": "^3.7.0",
    "bootstrap": "^4.1.3",
    "font-awesome": "^4.7.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.2",
    "react-animate-on-scroll": "^2.1.5",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.4",
    "scrollreveal": "^4.0.0",
    "smoothscroll-polyfill": "^0.4.3"
  },
  "scripts": {
    "build:scss": "node-sass src/components/styles/scss/main.scss src/components/styles/css/style.css",
    "watch:scss": "node-sass src/components/styles/scss/main.scss src/components/styles/css/style.css -w",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch:scss start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build:scss build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start:build": "serve -s build"
  }
}
